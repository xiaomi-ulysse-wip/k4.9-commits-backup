+static u8 checksum(u8 *data, u8 len)
+{
+	u8 i;
+	u16 sum = 0;
+
+	for (i = 0; i < len; i++)
+		sum += data[i];
+
+	sum &= 0xFF;
+
+	return (0xFF - sum);
+}
