+static int fg_read_byte(struct bq_fg_chip *bq, u8 reg, u8 *val)
+{
+	int ret;
+
+	if (bq->skip_reads) {
+		*val = 0;
+		return 0;
+	}
+
+	mutex_lock(&bq->i2c_rw_lock);
+	ret = __fg_read_byte(bq->client, reg, val);
+	mutex_unlock(&bq->i2c_rw_lock);
+
+	return ret;
+}
