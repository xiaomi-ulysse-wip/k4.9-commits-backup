+static int fg_dm_post_access(struct bq_fg_chip *bq)
+{
+	int ret;
+	int i = 0;
+
+
+	ret = fg_write_word(bq, bq->regs[BQ_FG_REG_CTRL],
+						FG_SUBCMD_SOFT_RESET);
+	if (ret < 0)
+		return ret;
+
+	msleep(100);
+
+	while(i++ < CFG_UPDATE_POLLING_RETRY_LIMIT) {
+		ret = fg_check_cfg_update_mode(bq);
