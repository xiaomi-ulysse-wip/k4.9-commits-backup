+	BQ2560X_VBUS_USB = REG08_VBUS_TYPE_USB,
+	BQ2560X_VBUS_ADAPTER = REG08_VBUS_TYPE_ADAPTER,
+	BQ2560X_VBUS_OTG = REG08_VBUS_TYPE_OTG,
