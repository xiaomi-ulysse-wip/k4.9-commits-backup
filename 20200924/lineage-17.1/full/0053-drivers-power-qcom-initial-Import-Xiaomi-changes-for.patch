+	s32 ret;
+
+	ret = i2c_smbus_write_word_data(client, reg, val);
+	if (ret < 0) {
+		pr_err("i2c write word fail: can't write 0x%02X to reg 0x%02X\n",
+				val, reg);
+		return ret;
+	}
+
+	return 0;
+}
+
+static int __fg_read_block(struct i2c_client *client, u8 reg, u8 *buf, u8 len)
+{
+	int ret;
+	struct i2c_msg msg[2];
+	int i;
+
+	msg[0].addr = client->addr;
+	msg[0].flags = 0;
+	msg[0].buf = &reg;
+	msg[0].len = 1;
+
+	msg[1].addr = client->addr;
+	msg[1].flags = I2C_M_RD;
+	msg[1].buf = buf;
+	msg[1].len = len;
+
+	for (i = 0; i < 3; i++) {
+		ret = i2c_transfer(client->adapter, msg, ARRAY_SIZE(msg));
+		if (ret >= 0)
+			return ret;
+		else
+			msleep(5);
+	}
+	return ret;
+}
+
+static int __fg_write_block(struct i2c_client *client, u8 reg, u8 *buf, u8 len)
+{
+	int ret;
+	struct i2c_msg msg;
+	u8 data[64];
+	int i = 0;
+
+	data[0] = reg;
+	memcpy(&data[1], buf, len);
+
+	msg.addr = client->addr;
+	msg.flags = 0;
+	msg.buf = data;
+	msg.len = len + 1;
+
+	for (i = 0; i < 3; i++) {
+		ret = i2c_transfer(client->adapter, &msg, 1);
+		if (ret >= 0)
+			return ret;
+		else
+			msleep(5);
+	}
+	return ret;
+}
