+static int fg_seal(struct bq_fg_chip *bq)
+{
+	int ret;
+	int retry = 0;
+
+	fg_get_seal_state(bq);
+
+	if (bq->seal_state == SEAL_STATE_SEALED)
+		return 0;
+	msleep(5);
+	ret = fg_write_word(bq, bq->regs[BQ_FG_REG_CTRL], FG_SUBCMD_SEAL);
+
+	if (ret < 0) {
+		pr_err("Failed to send seal command\n");
+		return ret;
+	}
+
+	while (retry++ < 1000) {
+		fg_get_seal_state(bq);
+		if (bq->seal_state == SEAL_STATE_SEALED)
+			return 0;
+		msleep(200);
+	}
+
+	return -1;
+}
