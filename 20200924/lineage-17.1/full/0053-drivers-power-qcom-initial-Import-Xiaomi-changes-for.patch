+#define __PACKED	__attribute__((packed))
+#else
+#error "Make sure structure cmd_t is packed"
+#endif
+
+typedef enum {
+	CMD_INVALID = 0,
+	CMD_R,	/* Read */
+	CMD_W,	/* Write */
+	CMD_C,	/* Compare */
+	CMD_X,	/* Delay */
