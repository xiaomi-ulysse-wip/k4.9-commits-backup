+static void spi_clock_set(struct gf_dev *gf_dev, int speed)
+{
+	long rate;
+	int rc;
+
+	rate = spi_clk_max_rate(gf_dev->core_clk, speed);
+	if (rate < 0) {
+		pr_info("%s: no match found for requested clock frequency:%d",
+				__func__, speed);
+		return;
+	}
+
+	rc = clk_set_rate(gf_dev->core_clk, rate);
+}
